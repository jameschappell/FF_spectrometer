d1: usercomponent, userTypeName="udipole", B=0.2;

d2: drift, l=0.5*m, aper1=12.5*mm;

d3: drift, l=0.5*m, aper1=20*mm;

l1: line=(d2,d1,d3);
use, l1;

beam, particle="e-",
      energy=2*GeV;


option, maximumStepLength=10*cm;

beam, distrType="userfile",
      distrFile="ebeam.dat",
      distrFileFormat="E[GeV]";